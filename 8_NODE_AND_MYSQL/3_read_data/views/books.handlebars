<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Title</th>
      <th scope="col">Pages Qty</th>
    </tr>
  </thead>
  <tbody>
    {{#each books}}
    <tr>
      <th scope="row">{{this.id}}</th>
      <td>{{this.title}}</td>
      <td>{{this.pageqty}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>