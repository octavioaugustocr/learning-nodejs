<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Title</th>
      <th scope="col">Page Quantity</th>
      <th scope="col">View</th>
    </tr>
  </thead>
  <tbody>
    {{#each books}}
    <tr>
      <th scope="row">{{this.id}}</th>
      <td>{{this.title}}</td>
      <td>{{this.pageqty}}</td>
      <td><a href="/book/{{this.id}}">View</a></td>
    </tr>
    {{/each}}
  </tbody>
</table>

<button><a href="/">Back</a></button>